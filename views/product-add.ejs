<%- include('./partials/header_logueado') %>

<!-- Novedades -->
<div class="container products-wrapper">
    <div class="row">
        <div class="col-12">
            <h2 class="products-title">Agregar producto</h2>
        </div>
        <!-- Show the "ultimos" products -->
        <form class="formularios" method="post" enctype="multipart/form-data">
            <div>
                <label class="campos" for="">Nombre del producto</label>
                <input type="text" name="nombre" id="nombre">
            </div> <br>

            <div>
                <label class="campos" for="">Imagen</label>
                <input type="file" name="photo" id="img">
            </div> <br>
            <div>
                <label class="campos">Descripcion</label>
                <input type="text" name="descripcion" id="descripcion">
            </div> <br>
             <br>
            <div>
                <button>Agregar producto</button>
            </div>
        </form>
    </div>
</div>
<%- include('./partials/footer.ejs')%>;